@model EntityLayer.Concrete.Feature
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <p class="card-description"> Yetenek Güncelleme Paneli </p>


            <form asp-controller="Feature" asp-action="Index" method="post">
                <input asp-for="FeatureID" hidden />
                <label>Başlık</label>
                <input asp-for="Header" class="form-control" />
                <br />
                <label>İsim</label>
                <input asp-for="Name" class="form-control" />
                <br />
                <label>Alt başlık</label>
                <input asp-for="Title" class="form-control" />
                <br />
                <button class="btn btn-block btn-lg btn-outline-light">Kaydet</button>
            </form>


        </div>
    </div>
</div>

