    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}

<div class="col-md-6">
  
        <div class="col-md-12">
            <input type="text" id="inpsame" placeholder="Ad"/>
            <input type="text" id="inpsurname" placeholder="Soyad"/>
