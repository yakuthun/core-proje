@model EntityLayer.Concrete.Contact
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}


<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <p class="card-description"> Alt İletişim Güncelleme Paneli </p>

            <form asp-controller="ContactSubPlace" asp-action="Index" method="post">
                <input asp-for="ContactID" hidden />
                <label>Başlık</label>
                <input asp-for="Title" class="form-control" />
                <br />
                <label>Açıklama</label>
                <input asp-for="Description" class="form-control" />
                <br />
                <label>Mail</label>
                <input asp-for="Mail" class="form-control" />
                <br />
                <label>Telefon</label>
                <input asp-for="Phone" class="form-control" />
                <br />
                <button class="btn btn-block btn-lg btn-outline-light">Kaydet</button>
            </form>


        </div>
    </div>
</div>

